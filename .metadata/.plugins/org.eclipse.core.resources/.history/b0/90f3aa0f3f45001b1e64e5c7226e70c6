package playingcoffee.core;

public class Opcode {

	private Op[] ops;
	
	private final Register registers;
	
	public Opcode(Op... ops) {
		this.ops = ops;
	}
	
	public void execute(Registers regs, MMU mmu) {
		for (Op op : ops)
			op.execute(regs, mmu);
	}
	
	public interface Op {
		
		public void execute(Registers regs, MMU mmu);
	
	}
	
	public class Read16BitOperand implements Op {

		@Override
		public void execute(Registers regs, MMU mmu) {
			
		}
		
	}
}
